				</Anchors>
				<Scripts>
					<OnClick>
						TranqRotationOptions_Reset();
					</OnClick>
				</Scripts>
			</Button>
			<Button name="$parentOkay" inherits="OptionsButtonTemplate" text="确定">
				<Anchors>
					<Anchor point="BOTTOM">
						<Offset>
							<AbsDimension x="0" y="15"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						TranqRotationOptions_Okay();
					</OnClick>
				</Scripts>
			</Button>
			<Button name="$parentCancel" inherits="OptionsButtonTemplate" text="取消">
				<Anchors>
					<Anchor point="BOTTOM">
						<Offset>
							<AbsDimension x="100" y="15"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						TranqRotationOptions_Close();
					</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnShow>
				PlaySound("igMainMenuOpen");
				TranqRotationOptions_Open();
			</OnShow>
			<OnHide>
				PlaySound("igMainMenuClose");
			</OnHide>
		</Scripts>
	</Frame>

